                  <operation source="field" to="output.out_AccountDetails_SF[1]/Website">temp.tmp_AcountDetails_SF.Website</operation>
                  <operation source="field" to="output.out_AccountDetails_SF[1]/AccountID">temp.tmp_AcountDetails_SF.Id</operation>
                  <operation source="formula" to="output.out_AccountDetails_SF">
