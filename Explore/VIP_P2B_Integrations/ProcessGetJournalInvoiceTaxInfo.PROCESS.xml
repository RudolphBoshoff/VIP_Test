                        <operation source="formula" to="output.out_SecurityDeposit_TaxAmount">
                           <expression language="XQuery">xs:decimal(util:iif(fn:starts-with($temp.tmp_InvoiceHeader.InvoiceType , "CR"), $temp.tmp_InvoiceItems_SecurityDeposit_Record.LineInvCur * $temp.tmp_InvoiceItems_SecurityDeposit_Record.VATperc, -1 * $temp.tmp_InvoiceItems_SecurityDeposit_Record.LineInvCur * $temp.tmp_InvoiceItems_SecurityDeposit_Record.VATperc ))</expression>
