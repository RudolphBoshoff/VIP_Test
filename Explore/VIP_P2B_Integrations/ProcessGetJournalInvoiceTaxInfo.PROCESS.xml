               <assignment id="kq911ub6">
                  <title>Assignment to tmp_InvoiceHeader, tmp_InvoiceItemsList etc.</title>
                  <operation source="query" to="temp.tmp_InvoiceHeader">
                     <query advanceQuery="true" from="AppConnectionVPCdbo:InvoiceHeader">
                        <where>ProformaNr = '{$input.in_ProformaNr}'</where>
