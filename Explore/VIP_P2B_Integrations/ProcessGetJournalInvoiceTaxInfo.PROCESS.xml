                        <operation source="formula" to="output.out_SecurityDeposit_TaxCode">
                           <expression language="XQuery">util:iif($temp.tmp_InvoiceItems_SecurityDeposit_Record.TaxCode='SE', 'SO',  $temp.tmp_InvoiceItems_SecurityDeposit_Record.TaxCode)</expression>
