   {$x/ProjectProfileCode}
   {$x/ProjectCurrency}
   {$x/YY1_Customer_PPH}
   {if( $temp.tmp_BpUuid_SAP = 'Not Found' or util:isNull($temp.tmp_BpUuid_SAP) or $temp.tmp_BpUuid_SAP = '')
