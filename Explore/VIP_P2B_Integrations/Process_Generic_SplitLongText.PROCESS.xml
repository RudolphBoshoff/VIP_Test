            <flow id="a">
               <start id="b">
                  <link id="lf7xe4b3" targetId="lf7xe49j"/>
               </start>
               <assignment id="lf7xe49j">
                  <title>Split text into words</title>
                  <operation source="formula" to="temp.tmp_WordsList">
                     <expression language="XQuery">fn:tokenize(fn:normalize-space($input.in_Text) , ' ')</expression>
                  </operation>
                  <operation source="formula" to="temp.tmp_TextNormalized">
                     <expression language="XQuery">fn:normalize-space($input.in_Text )</expression>
                  </operation>
                  <link id="lf82up4g" targetId="lf7xe4aa"/>
               </assignment>
               <container id="lf7xe4aa" type="exclusive">
                  <title>Word List</title>
                  <flow id="lf7xe4ae">
                     <assignment id="lf82up4i">
                        <title>Next Word</title>
                        <operation source="formula" to="temp.tmp_Word_Current">
                           <expression language="XQuery">list:head($temp.tmp_WordsList)</expression>
                        </operation>
                        <operation source="formula" to="temp.tmp_WordsList">
                           <expression language="XQuery">list:tail($temp.tmp_WordsList)</expression>
                        </operation>
                        <operation source="formula" to="temp.tmp_FlagContinue">
                           <expression language="XQuery">if (fn:string-length($temp.tmp_ShortTextCurrent||' '||$temp.tmp_Word_Current) &lt;= $input.in_MaxStringLength  
	and fn:string-length($temp.tmp_Word_Current) &gt; 0)
then (1)
else (0)</expression>
                        </operation>
                        <link id="lf82upfy" targetId="lf82up5e"/>
                     </assignment>
                     <container id="lf82up5e" type="exclusive">
                        <title>Add or New</title>
                        <flow id="lf82up5i">
                           <assignment id="lf82up60">
                              <title>Add</title>
                              <operation source="formula" to="temp.tmp_ShortTextCurrent">
                                 <expression language="XQuery">$temp.tmp_ShortTextCurrent||' '||$temp.tmp_Word_Current</expression>
                              </operation>
                              <operation source="formula" to="temp.tmp_Counter">
                                 <expression language="XQuery">$temp.tmp_Counter+1 </expression>
                              </operation>
                              <link id="lf82upe3" targetId="lf82upe1"/>
                           </assignment>
                           <jumpTo id="lf82upe1">
                              <link id="lf82upe2" targetId="lf7xe4aa"/>
                           </jumpTo>
                        </flow>
                        <flow id="lf82up5l">
                           <assignment id="lf82up8m">
                              <title>New</title>
                              <operation source="formula" to="temp.tmp_TextList">
                                 <expression language="XQuery">list:append($temp.tmp_TextList , fn:substring($temp.tmp_ShortTextCurrent,1,$input.in_MaxStringLength ) )</expression>
                              </operation>
                              <operation source="formula" to="temp.tmp_ShortTextCurrent">
                                 <expression language="XQuery">$temp.tmp_Word_Current </expression>
                              </operation>
                              <operation source="formula" to="temp.tmp_Counter2">
                                 <expression language="XQuery">$temp.tmp_Counter2 +1</expression>
                              </operation>
                              <operation source="formula" to="temp.tmp_FlagContinue">
                                 <expression language="XQuery">if ( fn:string-length($temp.tmp_Word_Current) &gt; 0)
then (1)
else (0)</expression>
                              </operation>
                              <link id="lf82updg" targetId="lf82updf"/>
                           </assignment>
                           <container id="lf82updf" type="exclusive">
                              <title>Word Empty Check</title>
                              <flow id="lf82updi">
                                 <jumpTo id="lf82upej">
                                    <link id="lf82upey" targetId="lf7xe4aa"/>
                                 </jumpTo>
                              </flow>
                              <flow id="lf82updl">
                                 <assignment id="lf82upgv">
                                    <title>Assignment to txt_list2</title>
                                    <operation source="formula" to="output.out_TextList">
                                       <expression language="XQuery">list:sequence($temp.tmp_TextList  )</expression>
                                    </operation>
                                    <link id="lf8asm27" targetId="lf82upeu"/>
                                 </assignment>
                                 <end id="lf82upeu">
                                    <title>End 1</title>
                                 </end>
                              </flow>
                              <link id="lf82updh" targetId="lf82updi" type="containerLink">
                                 <condition source="formula">
                                    <function name="equals">
                                       <arg name="left">{$temp.tmp_FlagContinue}</arg>
                                       <arg name="right">{1}</arg>
                                    </function>
                                 </condition>
                              </link>
                              <link id="lf82updk" targetId="lf82updl" type="containerLink"/>
                           </container>
                        </flow>
                        <link id="lf82up5h" targetId="lf82up5i" type="containerLink">
                           <condition source="formula">
                              <function name="equals">
                                 <arg name="left">{$temp.tmp_FlagContinue}</arg>
                                 <arg name="right">{1}</arg>
                              </function>
                           </condition>
                        </link>
                        <link id="lf82up5k" targetId="lf82up5l" type="containerLink"/>
                     </container>
                  </flow>
                  <flow id="lf7xe4ah">
                     <end id="lf82upa2"/>
                  </flow>
                  <link id="lf7xe4ad" targetId="lf7xe4ae" type="containerLink">
                     <condition source="formula">
                        <function name="not-empty">
                           <arg name="field">{$temp.tmp_WordsList}</arg>
                        </function>
                     </condition>
                  </link>
                  <link id="lf7xe4ag" targetId="lf7xe4ah" type="containerLink">
                     <condition source="formula">
                        <function name="empty">
                           <arg name="field">{$temp.tmp_WordsList}</arg>
                        </function>
                     </condition>
                  </link>
               </container>
            </flow>
         </process>
