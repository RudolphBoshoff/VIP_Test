                        </function>
                     </condition>
                  </link>
                  <link id="kzi2youq" targetId="kzi2youw" type="containerLink">
                     <condition source="formula">
                        <function name="unset">
                           <arg name="field">{$temp.tmp_InvoiceHeader}</arg>
                        </function>
                     </condition>
                  </link>
                  <link id="kzi2youf" targetId="kx6j9sac"/>
