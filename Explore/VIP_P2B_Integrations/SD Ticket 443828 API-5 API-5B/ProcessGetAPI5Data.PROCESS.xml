               <assignment id="krffjo7g">
                  <title>Fetch BankAccount, InvoiceHeader Info</title>
                  <operation source="list" to="temp.tmp_Matter_Array">
                     <query advanceQuery="false" from="AppConnectionVPCdbo:Matters">
                        <where>RefCode = '{$input.in_Customer_RefCode }'</where>
                     </query>
                  </operation>
                  <operation source="formula" to="temp.tmp_Matter">
                     <expression language="XQuery">list:head($temp.tmp_Matter_Array )</expression>
                  </operation>
                  <operation source="query" to="temp.tmp_BankAccounts">
                     <query advanceQuery="false" from="AppConnectionVPCdbo:BankAccounts">
                        <where>EntCode = '{$input.in_CompanyCode }' and CurCode = '{$input.in_CurrencyCode }'</where>
                     </query>
                  </operation>
                  <operation source="query" to="temp.tmp_InvoiceHeader">
                     <query advanceQuery="true" from="AppConnectionVPCdbo:InvoiceHeader">
                        <where>RefCode = '{$input.in_Customer_RefCode }' and (cast(InvoiceNr as varchar) = '{$input.in_DocumentReferenceID}' or substring(InvoiceRef, 1, 16) = '{$input.in_DocumentReferenceID}')</where>
                     </query>
                  </operation>
                  <operation source="query" to="temp.tmp_BankAccountsENTCODE">
                     <query advanceQuery="false" from="AppConnectionVPCdbo:BankAccounts">
                        <where>EntCode = '{$input.in_CompanyCode }' and SeqNr = 1</where>
                     </query>
                  </operation>
                  <link id="kzi2youe" targetId="kzi2youd"/>
