                        <operation source="formula" to="temp.tmp_TaxAmount_Dec_Sign">
                           <expression language="XQuery">util:iif($temp.tmp_TaxAmount_Calc_N &gt; 0, 1, -1 )</expression>
                        </operation>
                        <operation source="formula" to="temp.tmp_TaxAmount_Base_N">
                           <expression language="XQuery">fn:floor(abs($temp.tmp_TaxAmount_Calc_N ))</expression>
                        </operation>
                        <operation source="formula" to="temp.tmp_TaxAmount_Dec_N">
                           <expression language="XQuery">(abs($temp.tmp_TaxAmount_Calc_N) - abs($temp.tmp_TaxAmount_Base_N)) *100 </expression>
                        </operation>
                        <operation source="formula" to="temp.tmp_TaxAmount_Dec_N2">
                           <expression language="XQuery">(fn:floor($temp.tmp_TaxAmount_Dec_N) )*0.01</expression>
                        </operation>
                        <operation source="formula" to="temp.tmp_TaxAmount">
                           <expression language="XQuery">($temp.tmp_TaxAmount_Base_N + $temp.tmp_TaxAmount_Dec_N2) * $temp.tmp_TaxAmount_Dec_Sign </expression>
