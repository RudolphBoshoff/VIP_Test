    &lt;TaxAmount&gt;{$temp.tmp_TaxAmount} &lt;/TaxAmount&gt;
&lt;/root&gt;</expression>
                        </operation>
                        <operation source="formula" to="output.out_TaxCode_TaxAmount_Array">
                           <expression language="XQuery">list:append($output.out_TaxCode_TaxAmount_Array , util:iif($temp.tmp_TaxCode_TaxAmount='.',0,$temp.tmp_TaxCode_TaxAmount) )</expression>
