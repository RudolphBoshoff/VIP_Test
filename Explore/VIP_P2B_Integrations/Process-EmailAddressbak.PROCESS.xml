            <flow id="a">
               <start id="b">
                  <link id="kz3z8cq5" targetId="kz3z8cq4"/>
               </start>
               <assignment id="kz3z8cq4">
                  <title>Assignment to tmp_RefContacts</title>
                  <operation source="query" to="temp.tmp_RefContacts">
                     <query advanceQuery="true" from="AppConnectionVPCdbo:RefContacts">
                        <where>RefCode = '{$input.in_RefCode}'</where>
                     </query>
                  </operation>
                  <link id="kz3z8cqs" targetId="kz3z8cqr"/>
               </assignment>
               <assignment id="kz3z8cqr">
                  <title>Assignment to tmp_Email, tmp_Email1</title>
                  <operation source="formula" to="temp.tmp_Email">
                     <expression language="XQuery">for $x in fn:tokenize($temp.tmp_RefContacts.cText , ";") return util:iif(fn:starts-with($x, ";") and fn:ends-with($x, ";"), fn:substring($x, 2, fn:string-length($x)-2),util:iif(fn:starts-with($x, ";"), fn:substring-after($x, ";"), util:iif(fn:ends-with($x, ";"), fn:substring($x, 1, fn:string-length($x)-1), $x)))</expression>
                  </operation>
                  <operation source="formula" to="temp.tmp_Email1">
                     <expression language="XQuery">for $x in fn:tokenize($temp.tmp_RefContacts.cText , ";") return &lt;EmailAddress&gt;{$x}&lt;/EmailAddress&gt;</expression>
                  </operation>
                  <link id="kz3z8cr9" targetId="kz3z8cr8"/>
               </assignment>
               <assignment id="kz3z8cr8">
                  <title>Assignment to tmp_Email_PO</title>
                  <operation source="formula" to="temp.tmp_Email_PO">
                     <expression language="XQuery">&lt;root xmlns:m="urn:informatica:ae:xquery:json2xml:meta-data"&gt;
 {    if(not(util:isNull($temp.tmp_RefContacts.cText))) then (
       &lt;to_EmailAddress&gt;
       &lt;results m:isArray="true"&gt;
       &lt;EmailAddress&gt;{$temp.tmp_EmailAddressArray }&lt;/EmailAddress&gt;
            &lt;/results&gt;
       &lt;/to_EmailAddress&gt;
   )
   else ()
}
&lt;/root&gt;</expression>
                  </operation>
                  <link id="kz3z8cra" targetId="kz3z8cqi"/>
               </assignment>
               <assignment id="kz3z8cqi">
                  <title>Assignment to tmp_Count</title>
                  <operation source="formula" to="temp.tmp_Count">
                     <expression language="XQuery">fn:string-length($temp.tmp_RefContacts.cText ) - fn:string-length(fn:replace($temp.tmp_RefContacts.cText , ";", ""))</expression>
                  </operation>
                  <link id="kz3z8cqk" targetId="c"/>
               </assignment>
               <end id="c"/>
            </flow>
