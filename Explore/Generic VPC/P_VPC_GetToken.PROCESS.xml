               <service id="l0ki0ip8">
                  <title>batch file to generate token using CURL</title>
                  <serviceName>avShellService</serviceName>
                  <serviceGUID/>
                  <serviceInput>
                     <parameter name="command" source="constant">E:\CurlScript\gettoken.bat</parameter>
