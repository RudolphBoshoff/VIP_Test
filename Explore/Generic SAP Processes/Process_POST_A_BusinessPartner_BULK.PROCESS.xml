   &lt;HttpStatusCode&gt;{util:iif(fn:contains($output.faultInfo[1]/reason, 'already exists') or fn:contains($output.faultInfo[1]/reason, 'R1/286'),'202', $output.faultInfo[1]/code)||$output.o_HttpResponseStatusCode}&lt;/HttpStatusCode&gt;
   &lt;Detail&gt;{$output.o_Message ||$output.faultInfo[1]/detail }&lt;/Detail&gt;
   &lt;Reason&gt;{$output.faultInfo[1]/reason}&lt;/Reason&gt;
   &lt;Parameters&gt;
