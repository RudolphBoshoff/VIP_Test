            <flow id="a">
               <start id="b">
                  <link id="lfk3yzlt" targetId="lfk3yzls"/>
               </start>
               <assignment id="lfk3yzls">
                  <title>Assignment to tmp_API08</title>
                  <operation source="query" to="temp.tmp_API08">
                     <query advanceQuery="false" from="AppConnectionVPCvip:api08">
                        <where>BusinessPartner_BP = '{$input.in_RefCode}'</where>
                     </query>
                  </operation>
                  <link id="lfk3yznx" targetId="lfk3yznw"/>
               </assignment>
               <container id="lfk3yznw" type="exclusive">
                  <title>tmp_API08</title>
                  <flow id="lfk3yzo0">
                     <subflow id="lftotfbv">
                        <title>Split Refmaster Name</title>
                        <subflowGUID>86gc7kurfu0kqqKCTpI3SP</subflowGUID>
                        <subflowPath>Process_Generic_SplitLongText</subflowPath>
                        <runForEach>false</runForEach>
                        <input>
                           <parameter name="in_Text" source="field" updatable="true">temp.tmp_API08[1]/Name</parameter>
                           <parameter name="in_MaxStringLength" source="constant" updatable="true">40</parameter>
                        </input>
                        <outputDef>
                           <field name="out_TextList" type="objectlist">
                              <options>
                                 <option name="referenceTo">$type:string</option>
                                 <option hide="true" name="multiSelect">true</option>
                                 <option name="required">false</option>
                              </options>
                           </field>
                        </outputDef>
                        <link id="lgv0iqq8" targetId="lgv0iqq7"/>
                     </subflow>
                     <assignment id="lgv0iqq7">
                        <title>Set Output Entity ONLY</title>
                        <operation source="formula" to="temp.tmp_EI">
                           <expression language="XQuery">fn:normalize-space(fn:substring-before($temp.tmp_API08[1]/E_mail___Invoice ||";",';'))</expression>
                        </operation>
                        <operation source="formula" to="temp.tmp_EC">
                           <expression language="XQuery">fn:normalize-space(fn:substring-before($temp.tmp_API08[1]/E_mail___Invoice___CC ||";",';'))</expression>
                        </operation>
                        <operation source="formula" to="temp.tmp_ER">
                           <expression language="XQuery">fn:normalize-space(fn:substring-before($temp.tmp_API08[1]/AccountingClerkInternetAddress  ||";",';'))</expression>
                        </operation>
                        <operation source="formula" to="temp.tmp_EO">
                           <expression language="XQuery">fn:normalize-space(fn:substring-before($temp.tmp_API08[1]/Email__not_EI_or_EC_ ||";",';'))</expression>
                        </operation>
                        <operation source="formula" to="temp.tmp_ED">
                           <expression language="XQuery">fn:normalize-space(fn:substring-before($temp.tmp_API08[1]/Email___Default ||";",';'))</expression>
                        </operation>
                        <operation source="formula" to="output.out_AddressDetails">
                           <expression language="XQuery">&lt;POAddressDetails&gt;
   &lt;AddrNr&gt;{$temp.tmp_API08[1]/AddrNr/text() }&lt;/AddrNr&gt;
   &lt;CityName&gt;{$temp.tmp_API08[1]/CityName/text() }&lt;/CityName&gt;
   &lt;PostalCode&gt;{$temp.tmp_API08[1]/PostalCode/text()  }&lt;/PostalCode&gt;      
   &lt;Address&gt;{ $temp.tmp_API08[1]/address/text()  }&lt;/Address&gt;      
   &lt;Address2&gt;{$temp.tmp_API08[1]/address2/text()  }&lt;/Address2&gt;
   &lt;Address3&gt;{$temp.tmp_API08[1]/address3/text()  }&lt;/Address3&gt;
   &lt;Address4&gt;{$temp.tmp_API08[1]/address4/text()  }&lt;/Address4&gt;
   &lt;Address5&gt;{$temp.tmp_API08[1]/address5/text()  }&lt;/Address5&gt;
   &lt;Country&gt;{$temp.tmp_API08[1]/Country/text() }&lt;/Country&gt; 
   &lt;EmailAddress&gt;{	util:iif( util:isNull($temp.tmp_EI), 
   		  			util:iif( util:isNull($temp.tmp_EC),
                  	util:iif( util:isNull($temp.tmp_EO)
                    	,$temp.tmp_ED 
                    	,$temp.tmp_EO )
                        ,$temp.tmp_EC )
                        ,$temp.tmp_EI )}&lt;/EmailAddress&gt;
   &lt;EmailInvoice&gt;{$temp.tmp_EI }&lt;/EmailInvoice&gt;
   &lt;EmailInvoiceCC&gt;{$temp.tmp_EC }&lt;/EmailInvoiceCC&gt;
   &lt;EmailInvoiceReminder&gt;{$temp.tmp_ER }&lt;/EmailInvoiceReminder&gt;
   &lt;EmailOther&gt;{util:iif( util:isNull($temp.tmp_EO )
   						  ,$temp.tmp_ED 
                          ,$temp.tmp_EO)}&lt;/EmailOther&gt; 
   &lt;Language&gt;{switch ($temp.tmp_API08[1]/Language/text()) 
   				case fn:string-length( $temp.tmp_API08[1]/Language/text())&gt;0 return  $temp.tmp_API08[1]/Language/text()
                default return 'EN'}&lt;/Language&gt;   
   &lt;Active&gt;1&lt;/Active&gt;
   &lt;AddrType&gt;BI&lt;/AddrType&gt;
   &lt;SeqNr&gt;1&lt;/SeqNr&gt;    
&lt;/POAddressDetails&gt;

</expression>
                        </operation>
                        <link id="lgv0iqrq" targetId="lgv0iqrp"/>
                     </assignment>
                     <end id="lgv0iqrp">
                        <title>End 1</title>
                     </end>
                  </flow>
                  <flow id="lfk3yzo3">
