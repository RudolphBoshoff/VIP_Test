                  <title>Parameters with Logic</title>
                  <operation source="formula" to="temp.tmp_CashPlanningGroup">
                     <expression language="XQuery">util:iif($input.VF_AccountDetails[1]/AccountType='Affiliate','E4', util:iif($input.VF_AccountDetails[1]/ShippingCountry!=fn:substring($input.VF_AccountDetails[1]/Entity_Code__c , 1, 2),'E3','E2' ) )</expression>
                  </operation>
                  <operation source="query" to="temp.tmp_YY1_Customer_Segment_cco2">
                     <query advanceQuery="true" from="US-MSSQL-DB-VForce:LOV_Corresp">
                        <where>FieldNameVForce = '{'Account.Sector__c'}' and ValueCodeVforce = '{$input.VF_AccountDetails[1]/Sector__c }'</where>
                     </query>
                  </operation>
                  <operation source="field" to="temp.tmp_YY1_Customer_Segment_cco">temp.tmp_YY1_Customer_Segment_cco2[1]/ValueCodeSAP</operation>
                  <operation source="formula" to="temp.tmp_CustomerAccountAssignmentGroup">
                     <expression language="XQuery">util:iif($input.VF_AccountDetails[1]/AccountType='Affiliate','03', util:iif($input.VF_AccountDetails[1]/ShippingCountry!=fn:substring($input.VF_AccountDetails[1]/Entity_Code__c , 1, 2),'02','01' ) )</expression>
                  </operation>
                  <operation source="formula" to="temp.tmp_IncotermsLocation1">
                     <expression language="XQuery">util:iif(util:isNull($input.VF_AccountDetails[1]/ShippingCity ),'EX-WORKS',$input.VF_AccountDetails[1]/ShippingCity )</expression>
                  </operation>
                  <operation source="list" to="temp.tmp_CustomerSalesAreaTax_Corresp">
                     <query advanceQuery="false" from="US-MSSQL-DB-VForce:LOV_Corresp">
                        <where>FieldNameSAP = '{'A_CustomerSalesAreaTax.CustomerTaxCategory'}' and FieldNameVForce = '{'Opportunity.Legal_Billing_Entity__c'}' and ValueCodeVforce = '{$input.VF_Opportunity[1]/Legal_Billing_Entity__c}'</where>
                     </query>
                  </operation>
                  <operation source="formula" to="temp.tmp_SalesAreaTax">
                     <expression language="XQuery">for $ctg in $temp.tmp_CustomerSalesAreaTax_Corresp
return &lt;root xmlns:m="urn:informatica:ae:xquery:json2xml:meta-data"&gt; &lt;results m:isArray="true"&gt;
                	&lt;SalesOrganization&gt;{$input.VF_Opportunity[1]/Legal_Billing_Entity__c}&lt;/SalesOrganization&gt;
                    &lt;DistributionChannel&gt;10&lt;/DistributionChannel&gt;
                    &lt;Division&gt;10&lt;/Division&gt;
                    &lt;DepartureCountry&gt;{fn:substring($input.VF_Opportunity[1]/Legal_Billing_Entity__c, 1, 2) }&lt;/DepartureCountry&gt;
                    &lt;CustomerTaxCategory&gt;{$ctg/ValueCodeSAP/text()}&lt;/CustomerTaxCategory&gt;
                    &lt;CustomerTaxClassification&gt;{util:iif($input.VF_AccountDetails[1]/SBQQ__TaxExempt__c='Yes',0,1)}&lt;/CustomerTaxClassification&gt;
                &lt;/results&gt; &lt;/root&gt;
</expression>
                  </operation>
