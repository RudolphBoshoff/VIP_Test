   { if(not(util:isNull($input.VF_AccountDetails[1]/ShippingCountry))) then (
      &lt;results m:isArray="true"&gt;
         &lt;CityName&gt;{$input.VF_AccountDetails[1]/ShippingCity }&lt;/CityName&gt;
         &lt;PostalCode&gt;{$input.VF_AccountDetails[1]/ShippingPostalCode }&lt;/PostalCode&gt;
         &lt;Language&gt;EN&lt;/Language&gt;           
         &lt;StreetName&gt;{$input.VF_AccountDetails[1]/ShippingStreet}&lt;/StreetName&gt;
         &lt;Region&gt;{$input.VF_AccountDetails[1]/ShippingState} &lt;/Region&gt;
         &lt;Country&gt;{$input.VF_AccountDetails[1]/ShippingCountry }&lt;/Country&gt;
         { $temp.tmp_EmailAddress_BP_PO/element()}
         { $temp.tmp_Phone_BP_PO/element()}
         { $temp.tmp_Fax_BP_PO/element()}
         { $temp.tmp_URL_BP_PO/element()}
