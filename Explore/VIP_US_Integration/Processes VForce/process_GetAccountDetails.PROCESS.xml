            <flow id="a">
               <start id="b">
                  <link id="kwz1tnh9" targetId="kwkw5kts"/>
               </start>
               <assignment id="kwkw5kts">
                  <title>Assign Account Details</title>
                  <operation source="query" to="temp.tmp_AcountDetails_SF">
                     <query advanceQuery="false" from="AppConnSalesForce:Account">
                        <where>Id = '{$input.in_AccountID }'</where>
                     </query>
                  </operation>
                  <link id="ky1kmr7n" targetId="ky1kmr7m"/>
               </assignment>
               <assignment id="ky1kmr7m">
                  <title>GetFrom Correspondence Table</title>
                  <operation source="list" to="temp.tmp_CorrespTable">
                     <query advanceQuery="true" from="US-MSSQL-DB-VForce2:LOV_Corresp">
                        <where>FieldNameVForce = '{'Account.Entity_Code__c'}' and ValueCodeVForce = '{$temp.tmp_AcountDetails_SF.Entity_Code__c }'</where>
                     </query>
                  </operation>
                  <operation source="field" to="temp.tmp_Entity_Code__c">temp.tmp_CorrespTable[1]/ValueCodeSAP</operation>
                  <operation source="formula" to="output.Out_EntityCode_inScope">
                     <expression language="XQuery">not(util:isNull($temp.tmp_CorrespTable[1]/ValueCodeSAP)) </expression>
                  </operation>
                  <link id="ky1kmr7o" targetId="kx0k6v52"/>
               </assignment>
               <assignment id="kx0k6v52">
                  <title>GetAccountDetails</title>
                  <operation source="field" to="output.out_AccountDetails_SF[1]/AccountName">temp.tmp_AcountDetails_SF.Name</operation>
                  <operation source="field" to="output.out_AccountDetails_SF[1]/AccountType">temp.tmp_AcountDetails_SF.Type</operation>
                  <operation source="field"
                             to="output.out_AccountDetails_SF[1]/Billing_Contact_Email__c">temp.tmp_AcountDetails_SF.Billing_Contact_Email__c</operation>
                  <operation source="field" to="output.out_AccountDetails_SF[1]/Currency__c">temp.tmp_AcountDetails_SF.Currency__c</operation>
                  <operation source="field" to="output.out_AccountDetails_SF[1]/Entity_Code__c">temp.tmp_AcountDetails_SF.Entity_Code__c</operation>
                  <operation source="field" to="output.out_AccountDetails_SF[1]/Fax">temp.tmp_AcountDetails_SF.Fax</operation>
                  <operation source="field" to="output.out_AccountDetails_SF[1]/Payment_Term__c">temp.tmp_AcountDetails_SF.Payment_Term__c</operation>
                  <operation source="field" to="output.out_AccountDetails_SF[1]/Phone">temp.tmp_AcountDetails_SF.Phone</operation>
                  <operation source="field" to="output.out_AccountDetails_SF[1]/Sector__c">temp.tmp_AcountDetails_SF.Sector__c</operation>
                  <operation source="field" to="output.out_AccountDetails_SF[1]/ShippingCity">temp.tmp_AcountDetails_SF.ShippingCity</operation>
                  <operation source="field" to="output.out_AccountDetails_SF[1]/ShippingCountry">temp.tmp_AcountDetails_SF.ShippingCountry</operation>
                  <operation source="field" to="output.out_AccountDetails_SF[1]/ShippingPostalCode">temp.tmp_AcountDetails_SF.ShippingPostalCode</operation>
                  <operation source="field" to="output.out_AccountDetails_SF[1]/ShippingState">temp.tmp_AcountDetails_SF.ShippingState</operation>
                  <operation source="field" to="output.out_AccountDetails_SF[1]/ShippingStreet">temp.tmp_AcountDetails_SF.ShippingStreet</operation>
                  <operation source="field" to="output.out_AccountDetails_SF[1]/Website">temp.tmp_AcountDetails_SF.Website</operation>
                  <operation source="field" to="output.out_AccountDetails_SF[1]/AccountID">temp.tmp_AcountDetails_SF.Id</operation>
                  <operation source="formula" to="output.out_AccountDetails_SF">
                     <expression language="XQuery">&lt;PO-VFAccount&gt;
   &lt;AccountName&gt;{$temp.tmp_AcountDetails_SF.Name }&lt;/AccountName&gt;
   &lt;AccountType&gt;{$temp.tmp_AcountDetails_SF.Type }&lt;/AccountType&gt;
   &lt;Billing_Contact_Email__c&gt;{$temp.tmp_AcountDetails_SF.Billing_Contact_Email__c }&lt;/Billing_Contact_Email__c&gt;
   &lt;Fax&gt;{$temp.tmp_AcountDetails_SF.Fax }&lt;/Fax&gt;
   &lt;Website&gt;{$temp.tmp_AcountDetails_SF.Website } &lt;/Website&gt;
   &lt;Phone&gt;{$temp.tmp_AcountDetails_SF.Phone }&lt;/Phone&gt;
   &lt;X18_Digit_ID__c&gt;{$temp.tmp_AcountDetails_SF.X18_Digit_ID__c }&lt;/X18_Digit_ID__c&gt;
   &lt;ShippingCity&gt;{$temp.tmp_AcountDetails_SF.ShippingCity }&lt;/ShippingCity&gt;
   &lt;ShippingCountry&gt;{$temp.tmp_AcountDetails_SF.ShippingCountryCode }&lt;/ShippingCountry&gt;
   &lt;ShippingPostalCode&gt;{$temp.tmp_AcountDetails_SF.ShippingPostalCode }&lt;/ShippingPostalCode&gt;
   &lt;ShippingState&gt;{$temp.tmp_AcountDetails_SF.ShippingStateCode }&lt;/ShippingState&gt;
   &lt;ShippingStreet&gt;{$temp.tmp_AcountDetails_SF.ShippingStreet }&lt;/ShippingStreet&gt;
   &lt;Payment_Term__c&gt;{'NT'||$temp.tmp_AcountDetails_SF.Payment_Term__c }&lt;/Payment_Term__c&gt;
   &lt;Sector__c&gt;{$temp.tmp_AcountDetails_SF.Sector__c }&lt;/Sector__c&gt;
   &lt;Entity_Code__c&gt;{$temp.tmp_Entity_Code__c}&lt;/Entity_Code__c&gt; 
   &lt;Currency__c&gt;{$temp.tmp_AcountDetails_SF.Currency__c }&lt;/Currency__c&gt;
   &lt;AccountID&gt;{$temp.tmp_AcountDetails_SF.Id }&lt;/AccountID&gt;
   &lt;BillingContactFirstName__c&gt;{$temp.tmp_AcountDetails_SF.BillingContactFirstName__c }&lt;/BillingContactFirstName__c&gt;
   &lt;BillingContactLastName__c&gt;{$temp.tmp_AcountDetails_SF.BillingContactLastName__c }&lt;/BillingContactLastName__c&gt;
   &lt;Billing_Contact_Phone__c&gt;{$temp.tmp_AcountDetails_SF.Billing_Contact_Phone__c }&lt;/Billing_Contact_Phone__c&gt;
   &lt;BillingStreet&gt;{$temp.tmp_AcountDetails_SF.BillingStreet }&lt;/BillingStreet&gt;
   &lt;BillingCity&gt;{$temp.tmp_AcountDetails_SF.BillingCity }&lt;/BillingCity&gt;
   &lt;BillingState&gt;{$temp.tmp_AcountDetails_SF.BillingStateCode }&lt;/BillingState&gt;
   &lt;BillingPostalCode&gt;{$temp.tmp_AcountDetails_SF.BillingPostalCode }&lt;/BillingPostalCode&gt;
   &lt;BillingCountry&gt;{$temp.tmp_AcountDetails_SF.BillingCountryCode }&lt;/BillingCountry&gt;
   &lt;SBQQ__TaxExempt__c&gt;{$temp.tmp_AcountDetails_SF.SBQQ__TaxExempt__c }&lt;/SBQQ__TaxExempt__c&gt;
&lt;/PO-VFAccount&gt;


</expression>
                  </operation>
                  <link id="kx0k6v54" targetId="c"/>
               </assignment>
               <end id="c"/>
