                  </flow>
                  <flow id="l6g7v6yq">
                     <assignment id="l6kpk8fd">
                        <title>Evaluate Retry</title>
                        <operation source="formula" to="temp.tmp_Counter">
                           <expression language="XQuery">$temp.tmp_Counter +1</expression>
                        </operation>
                        <operation source="formula" to="temp.tmp_RetryFlag">
                           <expression language="XQuery">util:iif($temp.tmp_Counter &lt;=3 and $output.faultInfo[1]/code = 'HTTP_401',1,0) </expression>
                        </operation>
                        <operation source="formula" to="temp.tmp_ErrorDetails">
                           <expression language="XQuery">util:parseJSON($output.faultInfo[1]/reason)</expression>
                        </operation>
                        <link id="l6kpk8l5" targetId="l6kpk8l4"/>
                     </assignment>
                     <container id="l6kpk8l4" type="exclusive">
                        <title>Retry Flag</title>
                        <flow id="l6kpk8l8">
                           <wait id="l6kpk8rr">
                              <title>Wait</title>
                              <duration source="formula">
                                 <function name="duration_expression">
                                    <arg name="value">{$temp.tmp_Counter}</arg>
                                    <arg name="scale">S</arg>
                                    <arg name="businessDays"/>
