                                 </assignment>
                                 <link id="lhg23bmc" targetId="lhg23bm5" type="containerLink"/>
                              </flow>
                              <link id="lhg23bm7" targetId="lhg23bm8" type="containerLink">
                                 <condition source="formula">
                                    <function name="contains">
                                       <arg name="left">{$output.faultInfo[1]/reason}</arg>
                                       <arg name="right">R1/286</arg>
                                    </function>
                                 </condition>
                              </link>
