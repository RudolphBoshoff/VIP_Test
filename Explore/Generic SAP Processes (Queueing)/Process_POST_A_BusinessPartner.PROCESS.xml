                                    </operation>
                                    <operation source="field"
                                               to="output.out_ProcessVariables_POST_A_BusinessPartner[1]/Description">output.faultInfo[1]/reason</operation>
                                    <operation source="constant"
                                               to="output.out_ProcessVariables_POST_A_BusinessPartner[1]/FailedAtStep">SAP: Create a new Business Partner</operation>
                                 </assignment>
                                 <link id="lhg23bm9" targetId="lhg23bm5" type="containerLink"/>
                              </flow>
                              <flow id="lhg23bmb">
                                 <assignment id="lhg23bq4">
                                    <title>Add Error Response to PV</title>
                                    <operation source="field"
                                               to="output.out_ProcessVariables_POST_A_BusinessPartner[1]/Code">output.faultInfo[1]/code</operation>
                                    <operation source="field"
                                               to="output.out_ProcessVariables_POST_A_BusinessPartner[1]/Error">output.faultInfo[1]/reason</operation>
                                    <operation source="field"
                                               to="output.out_ProcessVariables_POST_A_BusinessPartner[1]/Description">output.faultInfo[1]/detail</operation>
